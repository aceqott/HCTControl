            android:layout_marginLeft="5dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Betatester"
            android:textSize="12dp"
            android:layout_toEndOf="@+id/imageViewGamberrete75"
            android:layout_marginLeft="5dp"
            android:layout_below="@+id/textGamberrete75" />
        <ImageView
            android:layout_width="1dp"
            android:layout_height="wrap_content"
            android:id="@+id/divisor2"
            android:layout_centerHorizontal="true" />
        <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/koko"
            android:id="@+id/imageViewkoko"
            android:baselineAlignBottom="false"
            android:layout_alignWithParentIfMissing="false"
            android:layout_alignParentBottom="false"
            android:layout_alignParentEnd="false"
            android:layout_alignParentStart="false"
            android:layout_toRightOf="@+id/divisor2"
            android:layout_marginLeft="5dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textkoko"
            android:text="Koko"
            android:textStyle="bold"
            android:textSize="16dp"
            android:layout_toEndOf="@+id/imageViewkoko"
            android:layout_marginLeft="5dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Betatester"
            android:textSize="12dp"
            android:layout_toEndOf="@+id/imageViewkoko"
            android:layout_marginLeft="5dp"
            android:layout_below="@+id/textkoko" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/fila3"
        android:layout_marginTop="10dp">
        <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/sumejorpista"
            android:id="@+id/imageViewSumejorpista"
            android:layout_marginLeft="10dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textSumejorpista"
            android:text="Sumejorpista"
            android:textStyle="bold"
            android:textSize="16dp"
            android:layout_toEndOf="@+id/imageViewSumejorpista"
            android:layout_marginLeft="5dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Betatester"
            android:textSize="12dp"
            android:layout_toEndOf="@+id/imageViewSumejorpista"
            android:layout_marginLeft="5dp"
            android:layout_below="@+id/textSumejorpista" />
        <ImageView
            android:layout_width="1dp"
            android:layout_height="wrap_content"
            android:id="@+id/divisor3"
            android:layout_centerHorizontal="true" />
        <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/escaliboor"
            android:id="@+id/imageViewEscaliboor"
            android:layout_toRightOf="@+id/divisor3"
            android:layout_marginLeft="5dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textEscaliboor"
            android:text="Escaliboor"
            android:textStyle="bold"
            android:textSize="16dp"
            android:layout_toEndOf="@+id/imageViewEscaliboor"
            android:layout_marginLeft="5dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Betatester"
            android:textSize="12dp"
            android:layout_toEndOf="@+id/imageViewEscaliboor"
            android:layout_marginLeft="5dp"
            android:layout_below="@+id/textEscaliboor" />
    </RelativeLayout>
</LinearLayout>